   "type": "object",
   "x-cap-file": {
      "fetch_from": "/url",
      "file_key": "/key"
   },
   "properties": {
      "url": {
         "type": "string",
         "pattern": "^(http|https|root)://"
      },
      "key": {
         "type": "string"
      },
      "version_id": {
         "type": "string"
      }
   }  
}
